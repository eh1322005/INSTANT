#(4):



list=[]
sum=0
n=int(input("please enter a number"))

for i in range(n):
  num=int(input("enter a number:"))

  if num>=0:
    list.append(num)
    sum+=num
  else:
    break
print("the sum of all the positive numbers is :",sum)
