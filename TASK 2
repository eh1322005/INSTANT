#(3):



a = int(input(" Enter age "))
b = int(input( "Enter age"))
c = int(input("Enter age "))
if a > b and a > c:
  print("A is the oldest")
elif b > a and b >c :
  print("B is the oldest")
else:
  print("C is the oldest")

if a < b and a < c:
  print("A is the youngest")
elif b < a and b <c :
  print("B is the youngest")
else:
  print("C is the youngest")
